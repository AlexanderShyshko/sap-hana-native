column table "SALES" (
    "ID" INTEGER unique not null GENERATED BY DEFAULT AS IDENTITY COMMENT 'Sale ID',
    "DISCOUNT_PERCENT" int COMMENT 'Discount precent',
    "DISCOUNT_START_DATE" date COMMENT 'Discount start date',
    "DISCOUNT_END_DATE" date COMMENT 'Discount end date',
    "PRODUCT_ID" int,
    "SALE_TYPE" varchar(50),
    PRIMARY KEY ("ID")
)
COMMENT 'Sales'