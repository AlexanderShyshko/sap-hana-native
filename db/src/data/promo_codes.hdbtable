column table "PROMO_CODES" (
    "ID" INTEGER unique not null GENERATED BY DEFAULT AS IDENTITY COMMENT 'Promo Code ID',
    "CODE" NVARCHAR(5) COMMENT 'Promo Code',
    "CLIENT_ID" int COMMENT 'Client ID',
    "ACTIVATION_STATUS" int COMMENT 'Activation status',
    PRIMARY KEY ("ID")
)
COMMENT 'Promo codes'