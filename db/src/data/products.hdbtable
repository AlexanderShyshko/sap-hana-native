column table "PRODUCTS" (
    "ID" INTEGER unique not null GENERATED BY DEFAULT AS IDENTITY COMMENT 'Shop ID',
    "PRODUCT_NAME" NVARCHAR(50) COMMENT 'Product name',
    "DESCRIPTION" NVARCHAR(512) COMMENT 'Product descritpion',
    "SHOP_ID" integer not null,
    PRIMARY KEY ("ID")
)
COMMENT 'Products'