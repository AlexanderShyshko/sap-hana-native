column table "PRODUCTS" (
    "ID" INTEGER unique not null GENERATED BY DEFAULT AS IDENTITY COMMENT 'Product ID',
    "PRODUCT_NAME" NVARCHAR(50) COMMENT 'Product name',
    "DESCRIPTION" NVARCHAR(512) COMMENT 'Product descritpion',
    "PRODUCT_PRICE" int COMMENT 'Product price',
    "SHOP_ID" integer not null COMMENT 'Shop ID',
    "PR_CATEGORY_ID" integer COMMENT 'Category ID',
    "SALE_ID" int COMMENT 'Sale ID',
    PRIMARY KEY ("ID")
)
COMMENT 'Products'