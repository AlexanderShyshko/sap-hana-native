column table "CLIENTS" (
    "ID" INTEGER unique not null GENERATED BY DEFAULT AS IDENTITY COMMENT 'Client ID',
    "FIRST_NAME" NVARCHAR(50) COMMENT 'First name',
    "LAST_NAME" NVARCHAR(50) COMMENT 'Last name',
    "CLIENT_EMAIL" NVARCHAR(50) COMMENT 'Email',
    "PHONE_NUMBER" NVARCHAR(50) COMMENT 'Phone number',
    PRIMARY KEY ("ID")
)
COMMENT 'Clients'