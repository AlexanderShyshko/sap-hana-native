PROCEDURE "edit_sale"(
    sale_id int,
    percent int,
    d_start_date date,
    d_end_date date,
    id_of_product int
  ) 
  LANGUAGE SQLSCRIPT AS 
BEGIN
UPDATE "SALES"
SET "DISCOUNT_PERCENT" = percent
WHERE "ID" = sale_id;

UPDATE "SALES"
SET "DISCOUNT_START_DATE" = d_start_date
WHERE "ID" = sale_id;

UPDATE "SALES"
SET "DISCOUNT_END_DATE" = d_start_date
WHERE "ID" = sale_id;

UPDATE "SALES"
SET "PRODUCT_ID" = id_of_product
WHERE "ID" = sale_id;
END