PROCEDURE "add_product"(
    name_of_product NVARCHAR(50),
    descr NVARCHAR(512),
    shop_id int,
    id_of_pr_category int
    
  ) 
  LANGUAGE SQLSCRIPT AS 
BEGIN
  INSERT INTO "PRODUCTS" (
      "PRODUCT_NAME",
      "DESCRIPTION",
      "SHOP_ID",
      "PR_CATEGORY_ID" 
    )
  VALUES(name_of_product, descr, shop_id, id_of_pr_category);
END